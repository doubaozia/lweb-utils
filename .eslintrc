{
  "extends": [
    "airbnb",
    "airbnb-typescript"
  ],
  "parser": "@typescript-eslint/parser",
  "plugins": [
    "@typescript-eslint"
  ],
  "env": {
    "browser": true,
    "node": true,
    "es6": true,
    "jest": true
  },
  "rules": {
    "no-underscore-dangle": [0],
    "import/no-unresolved": [0],
    "import/no-extraneous-dependencies": [0]
  },
  "parserOptions": {
    "project": "./tsconfig.json"
  }
}